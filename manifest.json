{
  "manifest_version": 2,
  "name": "Tab-Split",
  "version": "0.01",
  
  "description": "Two tabs become two tabs, only better.",

    "background": {
    "scripts": ["backgroundidea.js"]
    },

    "content_scripts": [
          {
            "matches": ["<all_urls>"],
            "js": ["newcontent.js"]
          }
    ],

  "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab"
  ]
}
